alias sl=ls

alias 'dir'='ls -la | more'
alias 'm'='mate'
alias 'o'='open'
alias gemdoc='gem environment gemdir'/doc
alias home="cd ~ ; ls"
alias cp="cp -i"
alias mv="mv -i"
alias del="/bin/rm -rf"
alias untar="tar -xvf"

alias up='cd ..;'
alias rtags="find . -name '*.rb' | xargs /usr/local/bin/ctags -a -e -f TAGS"
