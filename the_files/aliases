alias sl=ls
alias la='ls -lah'

alias 'dir'='ls -la | more'
alias 'o'='open'
alias gemdoc='gem environment gemdir'/doc
alias cp="cp -i"
alias mv="mv -i"

alias up='cd ..;'
alias rtags="find . -name '*.rb' | xargs /usr/local/bin/ctags -a -e -f TAGS"
alias search="find . | xargs grep "

alias claer='clear'
alias jss='./script/jruby_mingle_server'
alias ass='rake asset:compile_and_build_all'
alias mingle='cd ~/mingle'